{
  "name": "@voilajsx/appkit",
  "version": "1.0.3",
  "description": "A minimal, framework-agnostic Node.js application toolkit",
  "main": "src/index.js",
  "type": "module",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./src/index.js"
    },
    "./auth": {
      "types": "./types/auth/index.d.ts",
      "default": "./src/auth/index.js"
    },
    "./tenantdb": {
      "types": "./types/tenantdb/index.d.ts",
      "default": "./src/tenantdb/index.js"
    },
    "./cache": {
      "types": "./types/cache/index.d.ts",
      "default": "./src/cache/index.js"
    },
    "./events": {
      "types": "./types/events/index.d.ts",
      "default": "./src/events/index.js"
    },
    "./security": {
      "types": "./types/security/index.d.ts",
      "default": "./src/security/index.js"
    },
    "./error": {
      "types": "./types/error/index.d.ts",
      "default": "./src/error/index.js"
    },
    "./logging": {
      "types": "./types/logging/index.d.ts",
      "default": "./src/logging/index.js"
    },
    "./storage": {
      "types": "./types/storage/index.d.ts",
      "default": "./src/storage/index.js"
    },
    "./email": {
      "types": "./types/email/index.d.ts",
      "default": "./src/email/index.js"
    },
    "./queue": {
      "types": "./types/queue/index.d.ts",
      "default": "./src/queue/index.js"
    },
    "./config": {
      "types": "./types/config/index.d.ts",
      "default": "./src/config/index.js"
    },
    "./validation": {
      "types": "./types/validation/index.d.ts",
      "default": "./src/validation/index.js"
    },
    "./utils": {
      "types": "./types/utils/index.d.ts",
      "default": "./src/utils/index.js"
    }
  },
  "files": [
    "types/",
    "src/**/*.js",
    "!src/**/*.md",
    "!src/**/docs/**",
    "!src/**/tests/**",
    "!src/**/examples/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.js'",
    "build:types": "tsc",
    "prepublishOnly": "npm test && npm run build:types",
    "version": "npm test"
  },
  "keywords": [
    "nodejs",
    "node",
    "toolkit",
    "framework-agnostic",
    "auth",
    "authentication",
    "jwt",
    "bcrypt",
    "multi-tenant",
    "tenant",
    "database",
    "saas",
    "cache",
    "caching",
    "redis",
    "memcached",
    "events",
    "event-bus",
    "pubsub",
    "security",
    "csrf",
    "rate-limiting",
    "sanitization",
    "email",
    "smtp",
    "ses",
    "sendgrid",
    "mailgun",
    "queue",
    "job-queue",
    "background-jobs",
    "logging",
    "logger",
    "storage",
    "s3",
    "file-storage",
    "configuration",
    "config",
    "validation",
    "utils",
    "utilities",
    "express",
    "middleware",
    "error-handling"
  ],
  "author": "VoilaJS",
  "contributors": [
    {
      "name": "Krishna Teja GS",
      "email": "kt@voilacode.com",
      "url": "https://voilacode.com"
    }
  ],
  "creator": "Krishna Teja GS",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@vitest/coverage-v8": "^3.1.3",
    "esbuild": ">=0.25.0",
    "eslint": "^9.0.0",
    "express": "^5.1.0",
    "prettier": "^3.2.4",
    "supertest": "^7.1.1",
    "typescript": "^5.0.0",
    "vitest": "^3.1.3"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-ses": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@sendgrid/mail": "^8.1.5",
    "bull": "^4.10.0",
    "mailgun.js": "^9.0.0",
    "memcached": "^2.2.2",
    "mysql2": "^3.5.0",
    "nodemailer": "^6.9.0",
    "pg": "^8.11.0",
    "redis": "^4.7.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voilajsx/appkit.git"
  },
  "bugs": {
    "url": "https://github.com/voilajsx/appkit/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/voilajsx/appkit#readme"
}
